# Core Sui configuration
SUI_PRIVATE_KEY=your_sui_private_key_here
SUI_RPC_URL=https://fullnode.testnet.sui.io
SUI_GAS_BUDGET=20000000
SUI_BIN=sui

# On-chain battle configuration
BATTLE_PACKAGE_ID=0xYOUR_PACKAGE_ID
BATTLE_CONFIG_ID=0xYOUR_BATTLE_CONFIG_OBJECT
# Optionally override the event type directly (otherwise derived from package ID)
# BATTLE_REQUEST_EVENT_TYPE=0xYOUR_PACKAGE::monster_battle::BattleRequest
BATTLE_REQUEST_POLL_INTERVAL=12
BATTLE_REQUEST_BATCH_SIZE=5
BATTLE_LISTENER_CURSOR_FILE=.battle_listener.cursor

# Bridge / networking
NIMBUS_BRIDGE_URL=http://nimbus-bridge:3001
BRIDGE_PORT=3001

# Agent behaviour
AGENT_MODE=listener
